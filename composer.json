{
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Miles Chou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-dom": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/http": "^8",
        "laravel-zero/framework": "^8.8",
        "mileschou/imgecho": "^0.1.0",
        "symfony/css-selector": "^5.3",
        "symfony/dom-crawler": "^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["pastock"]
}
