{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/http": "^9",
        "laravel-zero/framework": "^9",
        "mileschou/imgecho": "^0.1.0",
        "pastock/pachart": "dev-master",
        "symfony/css-selector": "^6",
        "symfony/dom-crawler": "^6"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "pestphp/pest": "^1.22",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["pastock"]
}
